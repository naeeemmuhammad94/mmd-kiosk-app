{
  "name": "mmd-kiosk-app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start",
    "android:emulator": "expo start --android",
    "ios": "expo start --ios",
    "ios:iphone": "xcrun simctl boot \"iPhone 17 Pro Max\" 2>/dev/null; open -a Simulator; expo start --ios",
    "ios:ipad": "xcrun simctl boot \"iPad Pro 13-inch (M5)\" 2>/dev/null; open -a Simulator; expo start --localhost --ios",
    "ios:ipad-11": "xcrun simctl boot \"iPad Pro 11-inch (M5)\" 2>/dev/null; open -a Simulator; expo start --localhost --ios",
    "ios:ipad-mini": "xcrun simctl boot \"iPad mini (A17 Pro)\" 2>/dev/null; open -a Simulator; expo start --localhost --ios",
    "ios:ipad-air": "xcrun simctl boot \"iPad Air 13-inch (M3)\" 2>/dev/null; open -a Simulator; expo start --localhost --ios",
    "ios:ipad:tunnel": "xcrun simctl boot \"iPad Pro 13-inch (M5)\" 2>/dev/null; open -a Simulator; expo start --tunnel --ios",
    "ios:ipad-11:tunnel": "xcrun simctl boot \"iPad Pro 11-inch (M5)\" 2>/dev/null; open -a Simulator; expo start --tunnel --ios",
    "ios:ipad-mini:tunnel": "xcrun simctl boot \"iPad mini (A17 Pro)\" 2>/dev/null; open -a Simulator; expo start --tunnel --ios",
    "ios:ipad-air:tunnel": "xcrun simctl boot \"iPad Air 13-inch (M3)\" 2>/dev/null; open -a Simulator; expo start --tunnel --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "build:ios": "./scripts/build_ios.sh",
    "build:android": "./scripts/build_android.sh",
    "submit:ios": "./scripts/submit_ios.sh",
    "submit:android": "./scripts/submit_android.sh",
    "build:ios:staging": "./scripts/build_ios_staging.sh",
    "build:android:staging": "./scripts/build_android_staging.sh"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@hookform/resolvers": "^3.9.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@tanstack/react-query": "^5.62.11",
    "axios": "^1.13.2",
    "expo": "~54.0.0",
    "expo-asset": "~12.0.0",
    "expo-constants": "~18.0.13",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.0",
    "expo-secure-store": "~15.0.8",
    "expo-status-bar": "~3.0.9",
    "moment": "^2.30.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.53.2",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.12.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "^0.7.1",
    "react-native-worklets-core": "^1.6.2",
    "zod": "^3.23.8",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@testing-library/react-native": "^12.8.1",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-native": "^4.1.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "react-native-svg-transformer": "^1.5.2",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.5.0"
  },
  "private": true
}
